{
    "debug": true,
    "domain": "sdslabs.co",
    "secret": "YOUR_SECRET_KEY",
    "projectRoot": "/gasper",
    "rcFile": "Gasperfile.txt",
    "offlineMode": false,
    "dnsServers": ["8.8.8.8", "8.8.4.4"],
    "admin": {
        "email": "alphadose@gmail.com",
        "password": "alphadose",
        "username": "alphadose"
    },
    "cron": {
        "cleanupInterval": 600,
        "exposureInterval": 30,
        "dnsRecordUpdateInterval": 10
    },
    "mongo": {
        "url": "mongodb://localhost:27017"
    },
    "redis": {
        "host": "localhost",
        "port": 6379,
        "password": "",
        "DB": 0
    },
    "cloudflare": {
        "plugIn": false,
        "publicIP": "",
        "token": ""
    },
    "falcon": {
        "plugIn": false,
        "falconClientId": "",
        "falconClientSecret": "",
        "falconUrlAccessToken": "http://falcon.sdslabs.local/access_token",
        "falconUrlResourceOwnerDetails": "http://falcon.sdslabs.local/users/",
        "falconAccountsUrl": "http://arceus.sdslabs.local/",
        "redirectUri": ""
    },
    "images": {
        "static": "sdsws/static:2.0",
        "php": "sdsws/php:3.0",
        "nodejs": "sdsws/node:2.0",
        "python2": "sdsws/python2:1.0",
        "python3": "sdsws/python3:1.0",
        "mysql": "mysql:5.7",
        "mongodb": "mongo:latest"
    },
    "services": {
        "dominus": {
            "deploy": true,
            "port": 3000
        },
        "enrai": {
            "deploy": false,
            "port": 80,
            "ssl": {
                "plugIn": false,
                "port": 443,
                "certificate": "/home/user/fullchain.pem",
                "privateKey": "/home/user/privkey.pem"
            }
        },
        "mizu": {
            "deploy": true,
            "port": 4000
        },
        "hikari": {
            "deploy": false,
            "port": 53
        },
        "ssh": {
            "deploy": true,
            "port": 2222,
            "host_signers": ["/home/user/.ssh/id_rsa"],
            "using_passphrase": false,
            "passphrase": ""
        },
        "ssh_proxy": {
            "deploy": true,
            "port": 3222,
            "host_signers": ["/home/user/.ssh/id_rsa"],
            "using_passphrase": false,
            "passphrase": ""
        },
        "mysql": {
            "deploy": true,
            "port": 9000,
            "container_port": 33060,
            "env": {
                "MYSQL_ROOT_PASSWORD": "YOUR_MYSQL_PASSWORD"
            }
        },
        "mongodb": {
            "deploy": true,
            "port": 9001,
            "container_port": 27018,
            "env": {
                "MONGO_INITDB_ROOT_USERNAME": "YOUR_ROOT_NAME",
                "MONGO_INITDB_ROOT_PASSWORD": "YOUR_ROOT_PASSWORD"
            }
        }
    }
}
